            navigation: { nextEl: '.swiper-button-next', prevEl: '.swiper-button-prev' },
            autoplay: { delay: 5000 }
        });
        document.getElementById('menuToggle').addEventListener('click', function() {
            this.classList.toggle('active');
            const menu = document.querySelector('.header__menu._mobile');
            if (this.classList.contains('active')) {
                menu.style.display = 'block';
                menu.innerHTML = `<nav class="navigation"><a class="navigation__item" href="#about">О нас</a><a class="navigation__item" href="#training">Тренировки</a><a class="navigation__item" href="#coaches">Тренеры</a><a class="navigation__item" href="#prices">Абонементы</a><a class="navigation__item" href="#schedule">Расписание</a><a class="navigation__item" href="#news">Новости</a><a class="navigation__item" href="#gallery">Галерея</a><a class="navigation__item" href="#contacts">Контакты</a></nav>`;
            } else {
                menu.style.display = 'none';
                menu.innerHTML = '';
            }
        });
    </script>
</body>
</html>
